{
    "name": "ispserverfarm/unifi-meshviewer-generator",
    "description" : "Der Meshviewer Generator erzeugt die erforderlichen JSON Files zur Einbindung von Unifi AccessPoints, die mit der Stock Firmware Ã¼ber den Unifi Controller verwaltet werden. Der Generator holt mittels API die entsprechenden Daten vom Unifi Controller",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ubnt",
        "ubiquiti",
        "unifi",
        "controller",
        "api",
        "client",
        "meshviewer",
        "generator",
        "json"
    ],
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*",
        "art-of-wifi/unifi-api-client": "^1.1",
        "vlucas/phpdotenv": "^3.4",
        "composer/composer": "^1.9"
    },
    "require-dev": {
        "filp/whoops": "^2.5",
        "phpunit/phpunit": "^4.8.35 || ^5.0 || ^6.0 || ^7.0"
    },
    "homepage": "https://github.com/strausmann",
    "autoload": {
        "psr-4": {
            "ISPServerfarm\\UnifiMeshviewer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ISPServerfarm\\UnifiMeshviewer\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Bjoern Strausmann",
            "email": "bjoern@strausmann.net",
            "homepage": "https://www.isp-serverfarm.de"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
